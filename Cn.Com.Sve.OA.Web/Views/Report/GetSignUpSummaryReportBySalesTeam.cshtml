@model System.Collections.Generic.IEnumerable<Cn.Com.Sve.OA.Entities.SalesTeamSummaryReport>

@{
    Layout = null;
}
<!DOCTYPE HTML>
<html>
<head>
    <title></title>
    <meta name="viewport" content="width=device-width" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/Aqua/css/ligerui-all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/Gray/css/all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/ligerui-icons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/css/common.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.autocomplete.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Content/lib/jquery/jquery-1.7.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.widget.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.position.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.autocomplete.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/ligerUI/js/ligerui.all.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.metadata.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/messages_cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/js/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/js/LG.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/json2.js")" type="text/javascript"></script>
    <style type="text/css">
        body
        {
            font-size: 12px;
        }
        .l-table-edit
        {
        }
        .l-table-edit-td
        {
            padding: 4px;
        }
        .l-button-submit, .l-button-reset
        {
            width: 80px;
            float: left;
            margin-left: 10px;
            padding-bottom: 2px;
        }
        .l-verify-tip
        {
            left: 230px;
            top: 120px;
            z-index: 9999;
        }
    </style>
    <style>
	.ui-autocomplete-loading { background: white url('@Url.Content("~/Content/themes/base/images/ui-anim_basic_16x16.gif")') right center no-repeat; }
	#f_UserName { width: 25em; }
	</style>
    <script type="text/javascript">
    	var grd;
    	var $dialog;

    	$(function () {
    	});
    </script>
	<style type="text/css">
		.dt{ width:100%; border:1px solid #aaaaa;}
		.dt th { background-color:#eeeeee; text-align:center; font-weight:bold;}
	</style>
</head>
<body style="padding: 10px; height: 100%; text-align: center;">
    <table class="dt" border="1">
		<tr><th rowspan="2">小组名</th><th rowspan="2">学校数量</th><th rowspan="2">总资料量</th><th rowspan="2">已回访人数</th>
		<th rowspan="2">未回访量</th><th rowspan="2">电访量</th><th colspan="3">上门量</th>
		<th rowspan="2">定位</th><th colspan="3">报名量</th><th rowspan="2">按组员细化</th><th rowspan="2">按月份细化</th><th rowspan="2">按学校细化</th></tr>
		<tr><th>电话</th><th>常规</th><th>小计</th><th>电话</th><th>常规</th><th>小计</th></tr>
		@{ int t1 = 0, t2=0, t3=0, t4=0, t5=0, t6=0, t7=0,t8=0,t9=0,t10=0,t11=0; }
		@foreach (var item in this.Model) {
			t1 += item.Total;
			t2 += item.N;
			t3 += item.TelDropIn;
			t4 += item.NonTelDropIn;
			t5 += item.TotalDropIn;
			t6 += item.DinWei;
			t7 += item.TelSignUp;
			t8 += item.NonTelSignUp;
			t9 += item.TotalSignUp;
			t10 += item.SchoolQty;
			t11 += item.TelQty;
		<tr>
			<td>@item.SalesTeamName</td><td>@item.SchoolQty</td><td>@item.Total</td><td>@(item.Total-item.N)</td><td>@item.N</td><td>@item.TelQty</td><td>@item.TelDropIn</td><td>@item.NonTelDropIn</td><td>@item.TotalDropIn</td>
			<td>@item.DinWei</td><td>@item.TelSignUp</td><td>@item.NonTelSignUp</td><td>@item.TotalSignUp</td>
			<td><a href="javascript:top.f_addTab('tab-GetSignUpSummaryReportBySalesman-@item.SalesTeamId-@this.ViewBag.Year', '@item.SalesTeamName-按组员细化 @this.ViewBag.Year 年的招生量', 
				'@Url.Action("GetSignUpSummaryReportBySalesman", "Report")' + '?year=@this.ViewBag.Year&teamId=@item.SalesTeamId');">按组员细化</a></td>
			<td><a href="javascript:top.f_addTab('tab-GetSignUpSummaryReportBySalesTeamAndMonth-@item.SalesTeamId-@this.ViewBag.Year', '@item.SalesTeamName-按月份细化 @this.ViewBag.Year 年的招生量', 
				'@Url.Action("GetSignUpSummaryReportBySalesTeamAndMonth", "Report")' + '?year=@this.ViewBag.Year&teamId=@item.SalesTeamId');">按月份细化</a></td>
			<td><a href="javascript:top.f_addTab('tab-GetSignUpSummaryReportBySalesTeamAndSchool-@item.SalesTeamId-@this.ViewBag.Year', '@item.SalesTeamName-按学校细化 @this.ViewBag.Year 年的招生量', 
				'@Url.Action("GetSignUpSummaryReportBySalesTeamAndSchool", "Report")' + '?year=@this.ViewBag.Year&teamId=@item.SalesTeamId');">按学校细化</a></td>
		</tr>
		}
		<tr><td>合计</td><td>@t10</td><td>@t1</td><td>@(t1-t2)</td><td>@t2</td><td>@t11</td><td>@t3</td><td>@t4</td><td>@t5</td><td>@t6</td><td>@t7</td><td>@t8</td><td>@t9</td><td colspan="3"></td></tr>
	</table>
</body>
</html>
