<body style="padding: 10px; height: 100%; text-align: center;">
    <table class="dt" border="1">
		<tr><th>年度</th><th>学校</th><th>报名人数</th><th>未上报</th><th>已上报</th><th>未录取</th><th>已录取</th><th>录取不成功</th>
		<th>未缴费</th><th>已缴费</th><th>分期付款</th><th>未持证</th><th>已持证</th></tr>
		@foreach (var row in this.Model) {
		<tr>
			<td>@row[0]</td>
			<td>
				<a href="javascript:top.f_addTab('tab-GetQList-SumS-@row[0]-@row[1]', '@row[0]年 @row[1]','@Url.Action("ListStudents", "QualificationStudent")' + '?year=@row[0]&school=@Url.Encode(row[1].ToString())');">@row[1]</a>
			</td>
			<td>
