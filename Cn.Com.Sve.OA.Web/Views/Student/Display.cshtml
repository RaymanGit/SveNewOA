@model Cn.Com.Sve.OA.Entities.Student
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
    <title></title>
    <meta name="viewport" content="width=device-width" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/Aqua/css/ligerui-all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/Gray/css/all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/ligerUI/skins/ligerui-icons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/lib/css/common.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.autocomplete.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Content/lib/jquery/jquery-1.7.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.widget.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.position.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery/ui/jquery.ui.autocomplete.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/ligerUI/js/ligerui.all.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.metadata.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/messages_cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/jquery-validation/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/js/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/js/LG.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/lib/json2.js")" type="text/javascript"></script>
    <style type="text/css">
        body
        {
            font-size: 12px;
        }
        .l-table-edit
        {
        }
        .l-table-edit-td
        {
            padding: 4px;
        }
        .l-button-submit, .l-button-reset
        {
            width: 80px;
            float: left;
            margin-left: 10px;
            padding-bottom: 2px;
        }
        .l-verify-tip
        {
            left: 230px;
            top: 120px;
            z-index: 9999;
        }
    </style>
	<style type="text/css">
	.ui-autocomplete-loading { background: white url('@Url.Content("~/Content/themes/base/images/ui-anim_basic_16x16.gif")') right center no-repeat; }
	#f_UserName { width: 25em; }
	</style>
    <script type="text/javascript">
        $(function () {
            //$("#frmCondition").ligerForm();
        });
    </script>
</head>
<body style="padding: 10px; height: 100%; text-align: center;">
    <div style="width: 98%">
        <table style="border:1px solid black; width:1000px;" border="1">
            <tr><td colspan="8" style="background-color:#cccccc;">个人基本信息</td></tr>
            <tr>
                <td width="60px">姓名</td>
                <td width="190px">@this.Model.Name</td>
                <td width="60px">姓名拼音</td>
                <td width="190px">@this.Model.PinYin</td>
                <td width="60px">学号</td>
                <td width="190px">@this.Model.StudentNo</td>
                <td colspan="2" rowspan="10" align="center"><img src="@this.Model.Pic1" alt="照片" style="width:150;" width="150px" /></td>
            </tr>
            <tr>
                <td>性别</td>
                <td>@this.Model.Gender</td>
                <td>出生日期</td>
                <td>@String.Format("{0:D}",this.Model.Birthday)</td>
                <td>高考批次</td>
                <td>@this.Model.GaoKaoBatch</td>
            </tr>
            <tr>
                <td>学历</td>
                <td>@this.Model.EduLevel</td>
                <td>毕业时间</td>
                <td>@String.Format("{0:D}", this.Model.GraduateDate)</td>
                <td>高考成绩</td>
                <td>@this.Model.Score</td>
            </tr>
            <tr>
                <td>毕业学校</td>
                <td colspan="3">@((this.Model.GruduateSchool!=null)?this.Model.GruduateSchool.Name:"")</td>
                <td>专业</td>
                <td>@this.Model.Profession</td>
            </tr>
            <tr>
                <td>身份证号</td>
                <td colspan="3">@this.Model.IdCardNo</td>
                <td>户口类型</td>
                <td>@this.Model.HuKouType</td>
            </tr>
            <tr>
                <td>通讯地址</td>
                <td colspan="5">@this.Model.Address</td>
            </tr>
            <tr>
                <td>居住区域</td>
                <td colspan="3">@this.Model.FullDistrictName</td>
                <td>邮政编码</td>
                <td>@this.Model.Postcode</td>
            </tr>
            <tr>
                <td>费用来源</td>
                <td>@this.Model.FeeSource</td>
                <td>信息来源</td>
                <td>@(this.Model.InfoSourceSmall!=null?this.Model.InfoSourceSmall.FullName:"")</td>
                <td>咨询老师</td>
                <td>@this.Model.Consultants</td>
            </tr>
            <tr>
                <td width="60px">QQ号</td>
                <td width="190px">@this.Model.QQ</td>
                <td>电子邮箱</td>
                <td colspan="3">@this.Model.Email</td>
            </tr>
            <tr>
                <td>手机</td>
                <td>@this.Model.Mobile</td>
                <td>家庭电话</td>
                <td>@this.Model.HomeTelephone</td>
                <td>办公电话</td>
                <td>@this.Model.OfficeTelephone</td>
            </tr>
            <tr><td colspan="8" style="background-color:#cccccc;">学员家庭成员关系</td></tr>
            <tr>
                <td align="center">关系</td>
                <td align="center">姓名</td>
                <td align="center" colspan="3">工作单位</td>
                <td align="center" colspan="2">职位</td>
                <td align="center">手机号码</td>
            </tr>
            <tr>
                <td>@this.Model.MemberRelType1&nbsp;</td>
                <td>@this.Model.MemberName1&nbsp;</td>
                <td colspan="3">@this.Model.MemberCompany1&nbsp;</td>
                <td colspan="2">@this.Model.MemberPosition1&nbsp;</td>
                <td>@this.Model.MemberMobile1&nbsp;</td>
            </tr>
            <tr>
                <td>@this.Model.MemberRelType2&nbsp;</td>
                <td>@this.Model.MemberName2&nbsp;&nbsp;</td>
                <td colspan="3">@this.Model.MemberCompany2</td>
                <td colspan="2">@this.Model.MemberPosition2&nbsp;</td>
                <td>@this.Model.MemberMobile2&nbsp;</td>
            </tr>
            <tr>
                <td>@this.Model.MemberRelType3&nbsp;</td>
                <td>@this.Model.MemberName3&nbsp;</td>
                <td colspan="3">@this.Model.MemberCompany3&nbsp;</td>
                <td colspan="2">@this.Model.MemberPosition3&nbsp;</td>
                <td>@this.Model.MemberMobile3&nbsp;</td>
            </tr>
            <tr>
                <td>@this.Model.MemberRelType4&nbsp;</td>
                <td>@this.Model.MemberName4&nbsp;</td>
                <td colspan="3">@this.Model.MemberCompany4&nbsp;</td>
                <td colspan="2">@this.Model.MemberPosition4&nbsp;</td>
                <td>@this.Model.MemberMobile4&nbsp;</td>
            </tr>
            <tr><td colspan="8" style="background-color:#cccccc;">入学前相关经历</td></tr>
            <tr>
                <td colspan="2">学习/工作状况</td>
                <td colspan="6">@this.Model.WorkState</td>
            </tr>
            <tr>
                <td align="center" colspan="2">起止年月</td>
                <td align="center" colspan="4">学校或工作单位</td>
                <td align="center" colspan="2">职务</td>
            </tr>
            <tr>
                <td colspan="2">@this.Model.StudyDuration1&nbsp;</td>
                <td colspan="4">@this.Model.StudySchool1&nbsp;</td>
                <td colspan="2">@this.Model.StudyPosition1&nbsp;</td>
            </tr>
            <tr>
                <td colspan="2">@this.Model.StudyDuration2&nbsp;</td>
                <td colspan="4">@this.Model.StudySchool2&nbsp;</td>
                <td colspan="2">@this.Model.StudyPosition2&nbsp;</td>
            </tr>
            <tr>
                <td colspan="2">@this.Model.StudyDuration3&nbsp;</td>
                <td colspan="4">@this.Model.StudySchool3&nbsp;</td>
                <td colspan="2">@this.Model.StudyPosition3&nbsp;</td>
            </tr>
            <tr>
                <td>软件使用</td>
                <td>@this.Model.SoftwareExp</td>
                <td>软件编程</td>
                <td>@this.Model.ProgramExp</td>
                <td>网络操作</td>
                <td>@this.Model.NetworkExp</td>
                <td>学过相关课程</td>
                <td>@this.Model.RelCourse</td>
            </tr>
            <tr><td colspan="8" style="background-color:#cccccc;">资料提交情况</td></tr>
            <tr>
                <td align="center" colspan="2">照片电子档</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic1)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic1)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">四张彩照</td>
                <td align="center" colspan="2">@(this.Model.Receive1 == null ? "未提交" : this.Model.Receive1)</td>
            </tr>
            <tr>
                <td align="center" colspan="2">身份证正面扫描件</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic2)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic2)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">复印件</td>
                <td align="center" colspan="2">@(this.Model.Receive2 == null ? "未提交" : this.Model.Receive2)</td>
            </tr>
            <tr>
                <td align="center" colspan="2">身份证反面扫描件</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic3)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic3)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">复印件</td>
                <td align="center" colspan="2">@(this.Model.Receive3 == null ? "未提交" : this.Model.Receive3)</td>
            </tr>
            <tr>
                <td align="center" colspan="2">学历证件扫描件</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic4)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic4)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">复印件</td>
                <td align="center" colspan="2">@(this.Model.Receive4 == null ? "未提交" : this.Model.Receive4)</td>
            </tr>
            <tr>
                <td align="center" colspan="2">户口本首页扫描件</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic5)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic5)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">复印件</td>
                <td align="center" colspan="2">@(this.Model.Receive5 == null ? "未提交" : this.Model.Receive5)</td>
            </tr>
            <tr>
                <td align="center" colspan="2">户口本本人扫描件</td>
                <td align="center" colspan="2">
                    @if (!String.IsNullOrEmpty(this.Model.Pic6)) {
                    <a href="#" onclick="top.f_addTab('tab-display-pic-@this.Model.Id', '@this.Model.Name 的照片', '@Url.Action("DisplayPic", "Student")' + '?file=@this.Server.UrlEncode(this.Model.Pic6)');return false;">浏览</a>
                    } else {
                    @Html.Raw("未提交")
                    }
                </td>
                <td align="center" colspan="2">复印件</td>
                <td align="center" colspan="2">@(this.Model.Receive6 == null ? "未提交" : this.Model.Receive6)</td>
            </tr>
            <tr><td colspan="8" style="background-color:#cccccc;">学员在校相关信息</td></tr>
            <tr>
                <td>入学须知1</td>
                <td>@this.Model.Give1</td>
                <td>入学须知2</td>
                <td>@this.Model.Give2</td>
                <td>家长联系卡</td>
                <td>@this.Model.Give3</td>
                <td>笔记本电脑</td>
                <td>@this.Model.GiveNotebook</td>
            </tr>
            <tr>
                <td>保险购买</td>
                <td>@this.Model.InsuranceStatus</td>
                <td>报读学历</td>
                <td>@this.Model.IsGetQualification</td>
                <td>学历院校</td>
                <td colspan="3">@this.Model.QualificationSchool</td>
            </tr>
            <tr>
                <td>目前状况</td>
                <td>@this.Model.Status</td>
                <td>教材领用</td>
                <td>@this.Model.GiveCourseware</td>
                <td>入学时间</td>
                <td>@String.Format("{0:d}", this.Model.InSchoolDate)</td>
                <td>宿舍号</td>
                <td>@this.Model.DormNo</td>
            </tr>

            <tr>
                <td align="center" style="background-color:#eeeeee;">年级</td>
                <td align="center" style="background-color:#eeeeee;">班级</td>
                <td align="center" style="background-color:#eeeeee;">开班日期</td>
                <td align="center" style="background-color:#eeeeee;">开学典礼</td>
                <td align="center" style="background-color:#eeeeee;">结课日期</td>
                <td align="center" style="background-color:#eeeeee;">班主任</td>
                <td align="center" style="background-color:#eeeeee;">教员</td>
                <td align="center" style="background-color:#eeeeee;">责任领导</td>
            </tr>
			@foreach (var c in this.Model.ChangeClazzLogs) {
            <tr>
                <td align="center">@c.NewClazz.Semester</td>
                <td align="center">@c.NewClazz.Name</td>
                <td align="center">@String.Format("{0:d}", c.NewClazz.OpenDate)</td>
                <td align="center">@String.Format("{0:d}", c.NewClazz.KickOffDate)</td>
                <td align="center">@String.Format("{0:d}", c.NewClazz.ClosedDate)</td>
                <td align="center">@c.NewClazz.Master</td>
                <td align="center">@c.NewClazz.TeacherA @c.NewClazz.TeacherB</td>
                <td align="center">@c.NewClazz.Governor</td>
            </tr>
			}
            <tr><td colspan="8" style="background-color:#cccccc;">学员情况说明</td></tr>
            <tr>
                <td align="center" colspan="2">是否与学院老师有关系</td>
                <td align="center" colspan="2">@this.Model.HasRelTeacher</td>
                <td align="center" colspan="2">关系人</td>
                <td align="center" colspan="2">@this.Model.RelTeacher</td>
            </tr>
            <tr>
                <td align="center">家庭情况</td>
                <td align="center" colspan="7">@this.Model.HomeIntro</td>
            </tr>
            <tr>
                <td align="center">备注</td>
                <td align="center" colspan="7">@this.Model.Remark</td>
            </tr>

            <tr>
                <td align="center" colspan="8">
                    <a href="#" onclick="top.f_addTab('tab-edit-student-info-@this.Model.Id', '编辑 @this.Model.Name 的信息', '@Url.Action("Edit", "Student")' + '?id=@this.Model.Id');return false;">编辑</a>
				</td>
            </tr>
        </table>
    </div>
</body>
</html>
