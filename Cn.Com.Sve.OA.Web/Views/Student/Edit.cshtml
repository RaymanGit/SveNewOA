						<option value="已退学" @if (this.Model.Status != null && this.Model.Status.Equals("已退学")) { Html.Raw("selected='selected'"); }>已退学</option>
					</select>
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					教材领用
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					@if (this.Model.GiveCourseware != null && this.Model.GiveCourseware.Equals("已领")) {
					<input name='GiveCourseware' type='radio' value="已领" checked='checked'  />@Html.Raw("已领&nbsp;&nbsp;")
					<input name='GiveCourseware' type='radio' value="未领" />@Html.Raw("未领")
					} else {
					<input name='GiveCourseware' type='radio' value="已领"  />@Html.Raw("已领&nbsp;&nbsp;")
					<input name='GiveCourseware' type='radio' value="未领" checked='checked' />@Html.Raw("未领")
					}
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					宿舍号
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='DormNo' type='text' id='DormNo' class='txt' ltype='text' value="@this.Model.DormNo" />
				</td>
			</tr>
			<tr>
			</tr>
			<tr>
				<td colspan="8" style="background-color: #cccccc;">
					学员情况说明
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;' colspan="2">
					是否与学院老师有关系
				</td>
				<td class='l-table-edit-td' style='text-align: left;' colspan="2">
					@if (this.Model.HasRelTeacher != null && this.Model.HasRelTeacher.Equals("是")) {
					<input name='HasRelTeacher' type='radio' value="是" checked='checked'  />@Html.Raw("是&nbsp;&nbsp;")
					<input name='HasRelTeacher' type='radio' value="否" />@Html.Raw("否")
					} else {
					<input name='HasRelTeacher' type='radio' value="是"  />@Html.Raw("是&nbsp;&nbsp;")
					<input name='HasRelTeacher' type='radio' value="否" checked='checked' />@Html.Raw("否")
					}
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					关系人
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='RelTeacher' type='text' id='RelTeacher' class='txt' ltype='text' value="@this.Model.RelTeacher" />
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					入学日期
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='InSchoolDate' type='text' id='InSchoolDate' class='txt' ltype='text'  />
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					家庭情况
				</td>
				<td class='l-table-edit-td' style='text-align: left;' colspan="7">
					<input name='HomeIntro' type='text' id='HomeIntro' class='txt' ltype='text' value="@this.Model.HomeIntro" />
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					备注
				</td>
				<td class='l-table-edit-td' style='text-align: left;' colspan="7">
					<input name='Remark' type='text' id='Remark' class='txt' ltype='text' value="@this.Model.Remark" />
				</td>
			</tr>
			<tr>
				<td align="center" colspan="8">
					<input type="submit" id="btnProvinceSave" value="保存" class="l-button l-button-submit" />
				</td>
			</tr>
		</table>
	</div>
	</form>
</body>
</html>
