<body style="padding: 10px; height: 100%; text-align: center;">
	<form action="@Url.Action("Update","Student")" method="post" id="frmStudent" enctype="multipart/form-data">
	<input type="hidden" name="Id" id="Id" value="@this.Model.Id" />
	<input type="hidden" name="Pic1" id="Pic1" value="@this.Model.Pic1" />
	<input type="hidden" name="Pic2" id="Pic2" value="@this.Model.Pic2" />
	<input type="hidden" name="Pic3" id="Pic3" value="@this.Model.Pic3" />
	<input type="hidden" name="Pic4" id="Pic4" value="@this.Model.Pic4" />
	<input type="hidden" name="Pic5" id="Pic5" value="@this.Model.Pic5" />
	<input type="hidden" name="Pic6" id="Pic6" value="@this.Model.Pic6" />
	<input type="hidden" name="CreateTime" id="CreateTime" value="@this.Model.CreateTime" />
	<input type="hidden" name="NeedObtainWork" id="NeedObtainWork" value="@this.Model.NeedObtainWork" />

	<div style="width: 98%">
		<table style="border: 1px solid black; width: 1000px;" border="1">
			<tr>
				<td colspan="8" style="background-color: #cccccc;">
					个人基本信息
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right; width: 60px;'>
					姓名
				</td>
				<td class='l-table-edit-td' style='text-align: left; width: 190px'>
					<input name='Name' type='text' id='Name' class='txt yellowbg' ltype='text' value="@this.Model.Name" />
				</td>
				<td class='l-table-edit-td' style='text-align: right; width: 60px;'>
					姓名拼音
				</td>
				<td class='l-table-edit-td' style='text-align: left; width: 190px'>
					<input name='PinYin' type='text' id='PinYin' class='txt' ltype='text' value="@this.Model.PinYin" />
				</td>
				<td class='l-table-edit-td' style='text-align: right; width: 60px;'>
					学号
				</td>
				<td class='l-table-edit-td' style='text-align: left; width: 190px'>
					<input name='StudentNo' type='text' id='StudentNo' class='txt' ltype='text' value="@this.Model.StudentNo" />
				</td>
				<td colspan="2" rowspan="10" align="center" style="width: 150px;">
					&nbsp;<img src="@this.Model.Pic1" alt="照片" style="width:150;" width="150px" />
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					性别
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					@if (this.Model.Gender.Equals("男")) {
					<input name='Gender' type='radio' value="男" checked="checked" />@Html.Raw("男&nbsp;&nbsp;")
					<input name='Gender' type='radio' value="女" />@Html.Raw("女")
					} else {
					<input name='Gender' type='radio' value="男" />@Html.Raw("男&nbsp;&nbsp;")
					<input name='Gender' type='radio' value="女" checked="checked" />@Html.Raw("女")
					}
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					出生日期
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='Birthday' type='text' id='Birthday' class='txt' ltype='text' />
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					高考批次
				</td>
				
				<td class='l-table-edit-td' style='text-align: left; width: 190px'>
					<select id="GaoKaoBatch" name="GaoKaoBatch" style="width: 98%;">
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("落榜")) {
						<option value="落榜" selected='selected'>落榜</option>
					} else {
						<option value="落榜">落榜</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("1批")) {
						<option value="1批" selected='selected'>1批</option>
					} else {
						<option value="1批">1批</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("2A")) {
						<option value="2A" selected='selected'>2A</option>
					} else {
						<option value="2A">2A</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("2B")) {
						<option value="2B" selected='selected'>2B</option>
					} else {
						<option value="2B">2B</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("3A")) {
						<option value="3A" selected='selected'>3A</option>
					} else {
						<option value="3A">3A</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("3B")) {
						<option value="3B" selected='selected'>3B</option>
					} else {
						<option value="3B">3B</option>
					}
					@if (this.Model.GaoKaoBatch != null && this.Model.GaoKaoBatch.Equals("初中")) {
						<option value="初中" selected='selected'>初中</option>
					} else {
						<option value="初中">初中</option>
					}
					</select>
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					学历
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<select id="EduLevel" name="EduLevel" style="width: 98%;">
					@if (this.Model.EduLevel != null && this.Model.EduLevel.Equals("高中")) {
						<option value="高中" selected='selected'>高中</option>
					} else {
						<option value="高中">高中</option>
					}
					@if (this.Model.EduLevel != null && this.Model.EduLevel.Equals("初中")) {
						<option value="初中" selected='selected'>初中</option>
					} else {
						<option value="初中">初中</option>
					}
					@if (this.Model.EduLevel != null && this.Model.EduLevel.Equals("大专")) {
						<option value="大专" selected='selected'>大专</option>
					} else {
						<option value="大专">大专</option>
					}
					@if (this.Model.EduLevel != null && this.Model.EduLevel.Equals("本科")) {
						<option value="本科" selected='selected'>本科</option>
					} else {
						<option value="本科">本科</option>
					}
					@if (this.Model.EduLevel != null && this.Model.EduLevel.Equals("小学")) {
						<option value="小学" selected='selected'>小学</option>
					} else {
						<option value="小学">小学</option>
					}
					</select>
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					毕业时间
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='GraduateDate' type='text' id='GraduateDate' class='txt' ltype='text' />
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					高考成绩
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='Score' type='text' id='Score' class='txt' ltype='text' value="@this.Model.Score" />
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					毕业学校
				</td>
				<td class='l-table-edit-td' style='text-align: left;' colspan="3">
					<input name='GruduateSchoolName' type='text' id='GruduateSchoolName' class='txt' value="@((this.Model.GruduateSchool != null) ? this.Model.GruduateSchool.Name : "")"
						ltype='text' />
					<input name='GruduateSchoolId' type='hidden' id='GruduateSchoolId' value="@this.Model.GruduateSchoolId" />
				</td>
				<td class='l-table-edit-td' style='text-align: right;'>
					专业
				</td>
				<td class='l-table-edit-td' style='text-align: left;'>
					<input name='Profession' type='text' id='Profession' class='txt' ltype='text' value="@this.Model.Profession" />
				</td>
			</tr>
			<tr>
				<td class='l-table-edit-td' style='text-align: right;'>
					身份证号
				</td>
