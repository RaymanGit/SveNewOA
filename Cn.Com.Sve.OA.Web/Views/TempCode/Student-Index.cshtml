        function deleteStudent() {
            var row = gStudent.getSelectedRow();
            if (!row) { LG.tip('请选择行!'); return }

            $.ligerDialog.confirm("您确定要删除选中记录么？",function (r) { 
                if(r){
                    $.post("@Url.Action("Delete","Student")", { id: row.Id }
                        , function (result) {
                            if (result.Successful) {
                                gStudent.deleteSelectedRow();
                            } else {
                                $.ligerMessageBox.error('错误', result.Message);
                            }
                        }
                    );
                }
            });
        }
        function saveStudent(){
            if(action=="add"){
                $.post("@Url.Action("Add","Student")", $("#frmStudent").serialize(),
                    function (result) {
                        if (result.Successful) {
                            gStudent.addRow(result.Data);
                            $dialog.hidden();
                            LG.tip("操作成功！");
                        }else{
                            LG.showError(result.Message);
                        }
                    }
                );
            }else{
                $.post("@Url.Action("Update","Student")", $("#frmStudent").serialize(),
                    function (result) {
                        if (result.Successful) {
                            var row = gStudent.getSelected();
                            if (!row) { alert('请选择行'); return; }
                            gStudent.updateRow(row,result.Data);
                            $dialog.hidden();
                            LG.tip("操作成功！");
                        }else{
                            LG.showError(result.Message);
                        }
                    }
                );
            }
        }
